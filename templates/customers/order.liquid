{% layout 'customers_order' %}
      
  <div data-collapse="medium" data-animation="default" data-duration="400" role="banner" class="navigation w-nav">
    <div class="navigation-items"><a href="/" class="logo-link w-nav-brand"><img src="{{ 'janesiblue.png' | asset_url }}" width="220" alt="" class="logo-image"></a>
      <div class="navigation-wrap">
        <nav role="navigation" class="navigation-items w-nav-menu">{% for link in linklists['navigation-menu'].links %}<a href="{{ link.url }}" class="navigation-item w-nav-link">{{ link.title }}</a>{% endfor %}{% unless customer %}<a href="/account/login" class="login-item w-nav-link">{{ 'customers_order.A_0' | t }}</a>{% endunless %}{% if customer %}<a href="/account" class="login-item w-nav-link">{{ 'customers_order.A_1' | t }}</a>{% endif %}</nav>
        <div data-node-type="commerce-cart-wrapper" data-open-product="" data-wf-cart-type="modal" data-wf-cart-query="" data-wf-page-link-href-prefix="" class="w-commerce-commercecartwrapper"><a href="#" data-node-type="commerce-cart-open-link" class="w-commerce-commercecartopenlink button cc-cart w-inline-block"><svg class="w-commerce-commercecartopenlinkicon" width="17px" height="17px" viewBox="0 0 17 17"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><path d="M2.60592789,2 L0,2 L0,0 L4.39407211,0 L4.84288393,4 L16,4 L16,9.93844589 L3.76940945,12.3694378 L2.60592789,2 Z M15.5,17 C14.6715729,17 14,16.3284271 14,15.5 C14,14.6715729 14.6715729,14 15.5,14 C16.3284271,14 17,14.6715729 17,15.5 C17,16.3284271 16.3284271,17 15.5,17 Z M5.5,17 C4.67157288,17 4,16.3284271 4,15.5 C4,14.6715729 4.67157288,14 5.5,14 C6.32842712,14 7,14.6715729 7,15.5 C7,16.3284271 6.32842712,17 5.5,17 Z" fill="currentColor" fill-rule="nonzero"></path></g></svg><div class="w-inline-block">{{ 'customers_order.DIV_2' | t }}</div><div class="w-commerce-commercecartopenlinkcount cart-quantity-2" udy-el="cart-count">{{ cart.item_count }}</div></a>
          <div data-node-type="commerce-cart-container-wrapper" style="display:none" class="w-commerce-commercecartcontainerwrapper w-commerce-commercecartcontainerwrapper--cartType-modal">
            <div data-node-type="commerce-cart-container" class="w-commerce-commercecartcontainer">
              <div class="w-commerce-commercecartheader">
                <h4 class="w-commerce-commercecartheading">{{ 'customers_order.H4_3' | t }}</h4><a href="#" data-node-type="commerce-cart-close-link" class="w-commerce-commercecartcloselink w-inline-block"><svg width="16px" height="16px" viewBox="0 0 16 16"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g fill-rule="nonzero" fill="#333333"><polygon points="6.23223305 8 0.616116524 13.6161165 2.38388348 15.3838835 8 9.76776695 13.6161165 15.3838835 15.3838835 13.6161165 9.76776695 8 15.3838835 2.38388348 13.6161165 0.616116524 8 6.23223305 2.38388348 0.616116524 0.616116524 2.38388348 6.23223305 8"></polygon></g></g></svg></a></div>
              <div class="w-commerce-commercecartformwrapper">
                <form data-node-type="commerce-cart-form" class="w-commerce-commercecartform {% if cart.item_count == 0 %}udesly-display-none{% endif %}">
                  <ul role="list" class="mini-cart-list" udy-el="items-list">
                    {% for item in cart.items %}<li class="mini-cart-list-item">
                      <div class="product-row">
                        <div class="mini-cart-left-block"><a href="/cart/change?line={{ forloop.index }}&amp;amp;quantity=0" class="remove-product-link w-inline-block" udy-el="remove-item"></a>
                          <div class="mini-cart-image" style="background-image: url({{ item | img_url: 'master' }})"></div>
                          <div class="product-info-wrapper-2 mini-cart-info-wrapper"><a href="{{ item.product.url }}">{{ item.product.title }}</a>
                            <div class="text-block-79" href="/collections/vendors?q={{ item.vendor }}">{{ item.vendor }}</div>
                          </div>
                        </div>
                        <div class="price-wrapper">
                          <div class="mini-cart-price-wrapper">
                            <div class="text-block-78">{{ item.quantity }}</div>
                            <div class="text-block-76">{{ 'customers_order.DIV_4' | t }}</div>
                            <div class="text-block-78">{{ item.price | money }}</div>
                          </div>
                        </div>
                      </div>
                    </li>{% endfor %}
                  </ul>
                  
                  
                  <div class="w-commerce-commercecartfooter {% if cart.item_count == 0 %}udesly-display-none{% endif %}" udy-el="cart-actions">
                    <div class="w-commerce-commercecartlineitem">
                      <div>{{ 'customers_order.DIV_5' | t }}</div>
                      <div class="w-commerce-commercecartordervalue" udy-el="cart-total">{{ order.total_price | money }}</div>
                    </div>
                    <div class="checkout-actions"><a href="/cart" value="Cart" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton" data-loading-text="Hang Tight...">{{ 'customers_order.A_6' | t }}</a><a href="/checkout" value="Checkout" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton" data-loading-text="Hang Tight...">{{ 'customers_order.A_7' | t }}</a></div>
                  </div>
                </form>
                <div class="w-commerce-commercecartemptystate {% if cart.item_count != 0 %}udesly-display-none{% endif %}" udy-el="no-items-in-cart">
                  <div>{{ 'customers_order.DIV_8' | t }}</div>
                </div>
                <div style="display:none" data-node-type="commerce-cart-error" class="w-commerce-commercecarterrorstate">
                  <div class="w-cart-error-msg" data-w-cart-quantity-error="Product is not available in this quantity." data-w-cart-checkout-error="Checkout is disabled on this site." data-w-cart-general-error="Something went wrong when adding this item to the cart." data-w-cart-cart_order_min-error="Cart failed.">{{ 'customers_order.DIV_9' | t }}</div>
                </div>
              </div>
            </div>
          </div>
        <script id="mini-cart-template">
          window.udeslyMiniCartTemplate = function(item) {
            return `<li class="mini-cart-list-item" data-item-id="${item.id}">
                      <div class="product-row">
                        <div class="mini-cart-left-block"><a href="${item.remove}" class="remove-product-link w-inline-block" udy-el="remove-item"></a>
                          <div dimension="master" class="mini-cart-image" style="background-image: url(${item.image})"></div>
                          <div class="product-info-wrapper-2 mini-cart-info-wrapper"><a href="#">${item.title}</a>
                            <div class="text-block-79">${item.vendor}</div>
                          </div>
                        </div>
                        <div class="price-wrapper">
                          <div class="mini-cart-price-wrapper">
                            <div class="text-block-78">${item.quantity}</div>
                            <div class="text-block-76"> x</div>
                            <div class="text-block-78">${item.price}</div>
                          </div>
                        </div>
                      </div>
                    </li>`
          }
          </script></div>
        <div class="menu-button w-nav-button"><img src="{{ 'menu-icon_1menu-icon.png' | asset_url }}" width="22" alt="" class="menu-icon"></div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="main-heading-wrap">
      <div class="orders-wrapper">
        <h1 class="heading-29">{{ 'customers_order.H1_10' | t }}</h1>
        <div class="order-name---copy-this">{{ order.name }}</div>
      </div>
    </div>
    <div class="order-date---copy-this">{{ order.created_at | date: '%a, %b %d, %y' }}</div>
    <div class="orders-wrapper">
      <div class="text-block">{{ 'customers_order.DIV_11' | t }}</div>
      <div class="user-first-name---copy-this">{{ order.cancelled_at | date: '%a, %b %d, %y' }}</div>
    </div>
    <div class="orders-wrapper">
      <div class="text-block">{{ 'customers_order.DIV_12' | t }}</div>
      <div class="cancel-reason---copy-this">{{ order.cancel_reason_label }}</div>
    </div>
    <div class="divider"></div>
    <div class="address-wrapper">
      <div class="billing-wrapper">
        <div class="billing-address---copy-this">{{ order.billing_address | format_address }}</div>
      </div>
      <div class="shipping-wrapper">
        <div class="shipping-address---copy-this">{{ order.shipping_address | format_address }}</div>
      </div>
    </div>
    <div class="order-details-wrapper">
      <div class="order-details---copy-this">
        <div class="order-line">
          <div>{{ 'customers_order.DIV_13' | t }}</div>
          <div>{{ order.subtotal_price | money }}</div>
        </div>
        {% for discounts in order.discounts %}<div class="order-line">
          <div>{{ discount.code }}</div>
          <div>{{ 'customers_order.DIV_14' | t }}</div>
        </div>{% endfor %}
        <div class="order-line">
          <div>{{ shipping.title }}</div>
          <div>{{ shipping.price | money }}</div>
        </div>
        {% for tax_line in order.tax_lines %}<div class="order-line">
          <div>{{ tax_line.title }} ({{ tax_line.rate | times: 100 }}%)}</div>
          <div>{{ tax_line.price | money }}</div>
        </div>{% endfor %}
        <div class="order-line">
          <div>{{ 'customers_order.DIV_15' | t }}</div>
          <div>{{ order.total_price | money }}</div>
        </div>
      </div>
    </div>
  </div>
  <div class="section cc-subscribe-form">
    <div class="container cc-subscription-form">
      <div class="heading-jumbo-small">{{ 'customers_order.DIV_16' | t }}</div>
      <div class="paragraph-light cc-subscribe-paragraph">{{ 'customers_order.DIV_17' | t }}</div>
      <div class="form-block w-form">{% form 'contact' %}
        <div udesly-add-class="subscribe-form"></div><input type="email" class="text-field cc-subscribe-text-field w-input" maxlength="256" name="contact[email]" placeholder="Enter your email" id="email" required=""><input type="submit" value="Submit" data-wait="Please wait..." class="button w-button">
        
        
      </div><div class="form-block w-form">{% if form.errors %}<div class="status-message w-form-fail" style="display: inherit;">
          <div>{% include 'form-errors' %}</div>
        </div>{% endif %}{% if form.posted_successfully? %}<div class="status-message w-form-done" style="display: inherit;">
          <div>{{ 'customers_order.DIV_18' | t }}</div>
        </div>{% endif %}</div>{% endform %}
    </div>
  </div>
  <div class="section">
    <div class="section">
      <div class="container"></div>
    </div>
    <div class="container"></div>
    <div class="wrapper-3">
      <div class="section-7">
        <div class="w-layout-grid footer1-grid-2">
          <div id="w-node-5860412d4b66-87e29718" class="footer1-links-wrap">
            <div class="size6-text">{{ 'customers_order.DIV_19' | t }}</div><a href="https://www.janesicomfort.com/shop/janesi-comfort-gown-black" class="footer1-link-2">{{ 'customers_order.A_20' | t }}</a><a href="https://www.janesicomfort.com/blankets/blankets" class="footer1-link-2">{{ 'customers_order.A_21' | t }}</a></div>
          <div id="w-node-5860412d4b6d-87e29718" class="footer1-links-wrap">
            <div class="size6-text">{{ 'customers_order.DIV_22' | t }}</div><a href="https://www.janesicomfort.com/about" class="footer1-link-2">{{ 'customers_order.A_23' | t }}</a><a href="https://www.janesicomfort.com/contact-us" class="footer1-link-2">{{ 'customers_order.A_24' | t }}</a></div>
          <div id="w-node-5860412d4b74-87e29718" class="footer1-links-wrap">
            <div class="size6-text">{{ 'customers_order.DIV_25' | t }}</div><a href="https://www.instagram.com/janesi.comfort/" target="_blank" class="footer1-link-2">{{ 'customers_order.A_26' | t }}</a></div>
          <div id="w-node-5860412d4b79-87e29718" class="footer1-links-wrap">
            <div class="size6-text">{{ 'customers_order.DIV_27' | t }}</div><a href="https://www.janesicomfort.com/terms" class="footer1-link-2">{{ 'customers_order.A_28' | t }}</a><a href="https://www.janesicomfort.com/privacy-policy" class="footer1-link-2">{{ 'customers_order.A_29' | t }}</a><a href="https://www.janesicomfort.com/contact-us" class="footer1-link-2">{{ 'customers_order.A_30' | t }}</a></div>
          <div id="w-node-5860412d4b82-87e29718" class="footer1-links-wrap">
            <div class="size6-text">{{ 'customers_order.DIV_31' | t }}</div><a href="mailto:info@janesicomfort.com" class="footer1-link-2">{{ 'customers_order.A_32' | t }}</a><a href="tel:917920-4642" class="footer1-link-2">{{ 'customers_order.A_33' | t }}</a></div>
        </div>
      </div>
      <div class="section-8">
        <div class="w-layout-grid footer1-bottom-grid-2">
          <div id="w-node-5860412d4b8b-87e29718"><a href="http://pablodesigns.com/" target="_blank" class="footer1-bottom-link"> </a></div>
          <div id="w-node-5860412d4b8e-87e29718"><a href="https://webflow.com" class="footer1-bottom-link">{{ 'customers_order.A_34' | t }}</a></div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js?site=5ee8babd86c636bd4b669763" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
