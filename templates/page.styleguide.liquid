{% layout 'page.styleguide' %}
      
  <div data-collapse="medium" data-animation="default" data-duration="400" role="banner" class="navigation w-nav">
    <div class="navigation-items"><a href="/" class="logo-link w-nav-brand"><img src="{{ 'janesiblue.png' | asset_url }}" width="220" alt="" class="logo-image"></a>
      <div class="navigation-wrap">
        <nav role="navigation" class="navigation-items w-nav-menu">{% for link in linklists['navigation-menu'].links %}<a href="{{ link.url }}" class="navigation-item w-nav-link">{{ link.title }}</a>{% endfor %}{% unless customer %}<a href="/account/login" class="login-item w-nav-link">{{ 'styleguide.A_0' | t }}</a>{% endunless %}{% if customer %}<a href="/account" class="login-item w-nav-link">{{ 'styleguide.A_1' | t }}</a>{% endif %}</nav>
        <div data-node-type="commerce-cart-wrapper" data-open-product="" data-wf-cart-type="modal" data-wf-cart-query="" data-wf-page-link-href-prefix="" class="w-commerce-commercecartwrapper"><a href="#" data-node-type="commerce-cart-open-link" class="w-commerce-commercecartopenlink button cc-cart w-inline-block"><svg class="w-commerce-commercecartopenlinkicon" width="17px" height="17px" viewBox="0 0 17 17"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><path d="M2.60592789,2 L0,2 L0,0 L4.39407211,0 L4.84288393,4 L16,4 L16,9.93844589 L3.76940945,12.3694378 L2.60592789,2 Z M15.5,17 C14.6715729,17 14,16.3284271 14,15.5 C14,14.6715729 14.6715729,14 15.5,14 C16.3284271,14 17,14.6715729 17,15.5 C17,16.3284271 16.3284271,17 15.5,17 Z M5.5,17 C4.67157288,17 4,16.3284271 4,15.5 C4,14.6715729 4.67157288,14 5.5,14 C6.32842712,14 7,14.6715729 7,15.5 C7,16.3284271 6.32842712,17 5.5,17 Z" fill="currentColor" fill-rule="nonzero"></path></g></svg><div class="w-inline-block">{{ 'styleguide.DIV_2' | t }}</div><div class="w-commerce-commercecartopenlinkcount cart-quantity-2" udy-el="cart-count">{{ cart.item_count }}</div></a>
          <div data-node-type="commerce-cart-container-wrapper" style="display:none" class="w-commerce-commercecartcontainerwrapper w-commerce-commercecartcontainerwrapper--cartType-modal">
            <div data-node-type="commerce-cart-container" class="w-commerce-commercecartcontainer">
              <div class="w-commerce-commercecartheader">
                <h4 class="w-commerce-commercecartheading">{{ 'styleguide.H4_3' | t }}</h4><a href="#" data-node-type="commerce-cart-close-link" class="w-commerce-commercecartcloselink w-inline-block"><svg width="16px" height="16px" viewBox="0 0 16 16"><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd"><g fill-rule="nonzero" fill="#333333"><polygon points="6.23223305 8 0.616116524 13.6161165 2.38388348 15.3838835 8 9.76776695 13.6161165 15.3838835 15.3838835 13.6161165 9.76776695 8 15.3838835 2.38388348 13.6161165 0.616116524 8 6.23223305 2.38388348 0.616116524 0.616116524 2.38388348 6.23223305 8"></polygon></g></g></svg></a></div>
              <div class="w-commerce-commercecartformwrapper">
                <form data-node-type="commerce-cart-form" class="w-commerce-commercecartform {% if cart.item_count == 0 %}udesly-display-none{% endif %}">
                  <ul role="list" class="mini-cart-list" udy-el="items-list">
                    {% for item in cart.items %}<li class="mini-cart-list-item">
                      <div class="product-row">
                        <div class="mini-cart-left-block"><a href="/cart/change?line={{ forloop.index }}&amp;amp;quantity=0" class="remove-product-link w-inline-block" udy-el="remove-item"></a>
                          <div class="mini-cart-image" style="background-image: url({{ item | img_url: 'master' }})"></div>
                          <div class="product-info-wrapper-2 mini-cart-info-wrapper"><a href="{{ item.product.url }}">{{ item.product.title }}</a>
                            <div class="text-block-79" href="/collections/vendors?q={{ item.vendor }}">{{ item.vendor }}</div>
                          </div>
                        </div>
                        <div class="price-wrapper">
                          <div class="mini-cart-price-wrapper">
                            <div class="text-block-78">{{ item.quantity }}</div>
                            <div class="text-block-76">{{ 'styleguide.DIV_4' | t }}</div>
                            <div class="text-block-78">{{ item.price | money }}</div>
                          </div>
                        </div>
                      </div>
                    </li>{% endfor %}
                  </ul>
                  
                  
                  <div class="w-commerce-commercecartfooter {% if cart.item_count == 0 %}udesly-display-none{% endif %}" udy-el="cart-actions">
                    <div class="w-commerce-commercecartlineitem">
                      <div>{{ 'styleguide.DIV_5' | t }}</div>
                      <div class="w-commerce-commercecartordervalue" udy-el="cart-total">{{ cart.total_price | money }}</div>
                    </div>
                    <div class="checkout-actions"><a href="/cart" value="Cart" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton" data-loading-text="Hang Tight...">{{ 'styleguide.A_6' | t }}</a><a href="/checkout" value="Checkout" data-node-type="cart-checkout-button" class="w-commerce-commercecartcheckoutbutton" data-loading-text="Hang Tight...">{{ 'styleguide.A_7' | t }}</a></div>
                  </div>
                </form>
                <div class="w-commerce-commercecartemptystate {% if cart.item_count != 0 %}udesly-display-none{% endif %}" udy-el="no-items-in-cart">
                  <div>{{ 'styleguide.DIV_8' | t }}</div>
                </div>
                <div style="display:none" data-node-type="commerce-cart-error" class="w-commerce-commercecarterrorstate">
                  <div class="w-cart-error-msg" data-w-cart-quantity-error="Product is not available in this quantity." data-w-cart-checkout-error="Checkout is disabled on this site." data-w-cart-general-error="Something went wrong when adding this item to the cart." data-w-cart-cart_order_min-error="Cart failed.">{{ 'styleguide.DIV_9' | t }}</div>
                </div>
              </div>
            </div>
          </div>
        <script id="mini-cart-template">
          window.udeslyMiniCartTemplate = function(item) {
            return `<li class="mini-cart-list-item" data-item-id="${item.id}">
                      <div class="product-row">
                        <div class="mini-cart-left-block"><a href="${item.remove}" class="remove-product-link w-inline-block" udy-el="remove-item"></a>
                          <div dimension="master" class="mini-cart-image" style="background-image: url(${item.image})"></div>
                          <div class="product-info-wrapper-2 mini-cart-info-wrapper"><a href="#">${item.title}</a>
                            <div class="text-block-79">${item.vendor}</div>
                          </div>
                        </div>
                        <div class="price-wrapper">
                          <div class="mini-cart-price-wrapper">
                            <div class="text-block-78">${item.quantity}</div>
                            <div class="text-block-76"> x</div>
                            <div class="text-block-78">${item.price}</div>
                          </div>
                        </div>
                      </div>
                    </li>`
          }
          </script></div>
        <div class="menu-button w-nav-button"><img src="{{ 'menu-icon_1menu-icon.png' | asset_url }}" width="22" alt="" class="menu-icon"></div>
      </div>
    </div>
  </div>
  <div class="section">
    <div class="styleguide-header-wrap">
      <div class="heading-jumbo">{{ 'styleguide.DIV_10' | t }}</div>
      <div class="paragraph-bigger cc-bigger-light">{{ 'styleguide.DIV_11' | t }}</div>
    </div>
    <div class="container">
      <div class="styleguide-content-wrap">
        <div class="styleguide-block">
          <div class="label cc-styleguide-label">{{ 'styleguide.DIV_12' | t }}</div>
          <div class="heading-jumbo">{{ 'styleguide.DIV_13' | t }}</div>
          <div class="heading-jumbo-small">{{ 'styleguide.DIV_14' | t }}</div>
          <div class="heading-jumbo-tiny">{{ 'styleguide.DIV_15' | t }}</div>
          <h1>{{ 'styleguide.H1_16' | t }}</h1>
          <h2>{{ 'styleguide.H2_17' | t }}</h2>
          <h3>{{ 'styleguide.H3_18' | t }}</h3>
          <h4>{{ 'styleguide.H4_19' | t }}</h4>
          <h5>{{ 'styleguide.H5_20' | t }}</h5>
          <h6>{{ 'styleguide.H6_21' | t }}</h6>
        </div>
        <div class="divider"></div>
        <div class="styleguide-block">
          <div class="label cc-styleguide-label">{{ 'styleguide.DIV_22' | t }}</div>
          <p class="paragraph-bigger">{{ 'styleguide.P_23' | t }}</p>
          <p>{{ 'styleguide.P_24' | t }}</p>
          <p class="paragraph-light">{{ 'styleguide.P_25' | t }}</p>
          <p class="paragraph-small">{{ 'styleguide.P_26' | t }}</p>
          <p class="paragraph-tiny">{{ 'styleguide.P_27' | t }}</p>
          <div class="label">{{ 'styleguide.DIV_28' | t }}</div><a href="#">{{ 'styleguide.A_29' | t }}</a></div>
        <div class="divider"></div>
        <div class="styleguide-block">
          <div class="label cc-styleguide-label">{{ 'styleguide.DIV_30' | t }}</div>
          <div class="styleguide-button-wrap">
            <a href="#" class="button w-inline-block">
              <div class="text-block-97">{{ 'styleguide.DIV_31' | t }}</div>
            </a>
          </div>
          <div class="styleguide-button-wrap">
            <a href="#" class="button cc-jumbo-button w-inline-block">
              <div class="text-block-98">{{ 'styleguide.DIV_32' | t }}</div>
            </a>
          </div>
        </div>
        <div class="divider"></div>
        <div class="styleguide-block">
          <div class="label cc-styleguide-label">{{ 'styleguide.DIV_33' | t }}</div>
          <div class="rich-text w-richtext">
            <h2>{{ 'styleguide.H2_34' | t }}</h2>
            <p>{{ 'styleguide.P_35' | t }}</p>
            <blockquote>{{ 'styleguide.BLOCKQUOTE_36' | t }}</blockquote>
            <h4>{{ 'styleguide.H4_37' | t }}</h4>
            <p>{{ 'styleguide.P_38' | t }}</p>
            <figure style="max-width:1306px" id="w-node-7c899ca68d66-14669768" class="w-richtext-align-fullwidth w-richtext-figure-type-image">
              <div><img src="{{ 'placeholder-1_1.svg' | asset_url }}" alt=""></div>
              <figcaption>{{ 'styleguide.FIGCAPTION_39' | t }}</figcaption>
            </figure>
            <h4>{{ 'styleguide.H4_40' | t }}</h4>
            <p>{{ 'styleguide.P_41' | t }}</p>
            <ul role="list">
              <li>{{ 'styleguide.LI_42' | t }}</li>
              <li>{{ 'styleguide.LI_43' | t }}</li>
              <li>{{ 'styleguide.LI_44' | t }}</li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.4.1.min.220afd743d.js?site=5ee8babd86c636bd4b669763" type="text/javascript" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{% include 'notifications' %}{{ 'udesly-shopify.min.js' | asset_url | script_tag }}{{ 'option_selection.js' | shopify_asset_url | script_tag }}